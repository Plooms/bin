#!/bin/bash
# Check to see if an HDD is unmounted
SCRIPT_NAME="c.ln2tb-mount-check"

# Source the VAR MOAS
source /usr/local/bin/VAR-SCRIPT



# Start Lock
_LOCK_ON


export DISPLAY=:0.0


RUN_TIMES="0"


while [[ $RUN_TIMES < 2 ]] ;do

/usr/local/bin/c.ln2tb-mount-check-1
/usr/local/bin/c.ln2tb-mount-check-2
/usr/local/bin/c.ln2tb-mount-check-3
/usr/local/bin/c.ln2tb-mount-check-4
sleep 5


	RUN_TIMES=$((RUN_TIMES+1))

done



# End Lock
_LOCK_OFF
